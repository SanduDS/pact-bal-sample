{
  "consumer": {
    "name": "BallerinaClient"
  },
  "provider": {
    "name": "BallerinaProvider"
  },
  "interactions": [
    {
      "description": "Get greeting from ballerina service",
      "request": {
        "method": "GET",
        "path": "/hello"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "Hello world"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}